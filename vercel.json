{
  "version": 2,
  "outputDirectory": "build",
  "installCommand": "npm install",
  "env": {
    "CI": "false"
  },
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/$1"
    },
    {
      "src": "/static/(.*)",
      "dest": "/static/$1"
    },
    {
      "src": "/reports/(.*)",
      "dest": "/reports/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
} 